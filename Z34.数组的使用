import java.util.Scanner;
public class h{
	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		int n = cin.nextInt();
		int arr[][] = new int [n][];
		for(int i = 0;i < arr.length;i++) {
			arr[i] = new int [i + 1];
			for(int j = 0; j < arr[i].length;j++) {
				arr[i][j] = i + 1;
			}
		}
		for(int i = 0; i < arr.length;i++) {
			for(int j = 0; j < arr[i].length;j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
}
