import java.util.Scanner;
public class h{
	public static void main(String[] args) {
		int arr1[] = {1, 2, 3};
		Scanner scanner = new Scanner(System.in);
		int arr2[] = new int[arr1.length + 1];
		for(int i = 0; i < arr1.length;i++) {
			arr2[i] = arr1[i];
		}
		arr2[arr2.length - 1] = 5;
		arr1 = arr2;
		for(int i = 0;i < arr1.length;i++) {
			System.out.print(arr1[i] + " ");
		}
	}
}
