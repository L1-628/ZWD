import java.util.*;
public class Main{
	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		int m = cin.nextInt(),n = cin.nextInt();
		int A[][] = new int [m][n];
		
		for(int i = 0;i < m;i++){
			for(int j = 0;j< n; j++) {
				A[i][j]=cin.nextInt();
			}
		}
		for(int i = 0;i < m;i++){
			for(int j = 0;j< n; j++) {
				A[i][j] += cin.nextInt();
				System.out.print(A[i][j] +" ");
			}
			System.out.println();
		}
		cin.close();
	}
}
