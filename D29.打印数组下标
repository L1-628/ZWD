import java.util.Scanner;
public class h{
	public static void main(String[] args) {
		int arr[] = {4, -1, 9, 10, 23};
		int max = Integer.MIN_VALUE;
		int i = 0;
		int j = 0;
		for(i = 0; i < arr.length; i++) {
			if(arr[i] > max) {
				max = arr[i];
				j = i;
			}
		}
		System.out.println(max+" "+j+" ");
	}
}
